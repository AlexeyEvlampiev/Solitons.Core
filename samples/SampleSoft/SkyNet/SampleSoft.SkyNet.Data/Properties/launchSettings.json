{
  "profiles": {
    "localhost: Create|Upgrade": {
      "commandName": "Project",
      "commandLineArgs": "skynetdb create --server localhost --port 5433 --secrets ~/skynet/secrets.db:localhost"
    },
    "localhost: Upgrade": {
      "commandName": "Project",
      "commandLineArgs": "skynetdb update --secrets ~/skynet/secrets.db:localhost"
    },
    "localhost: Init Secrets": {
      "commandName": "Project",
      "commandLineArgs": "secrets init ~/skynet/secrets.db:localhost"
    }
  }
}